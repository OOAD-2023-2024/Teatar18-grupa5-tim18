@page "{id:int}"
@model KupovinaModel

@{
    ViewData["Title"] = "Placanje";
    ViewData["ActivePage"] = ManageNavPages.Kupovina;
}

<div class="row">
    <div class="col-md-6">
        <h3>@ViewData["Title"]</h3>
        <partial name="_StatusMessage" for="StatusMessage" />
        <form method="post">
            <div>
                <h5>Metode placanja i ostalo...</h5>
            </div>
            <div>
                <button type="submit" class="btn btn-primary">Plati</button>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}